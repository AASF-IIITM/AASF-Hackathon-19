    <!-- Patient Area Starts -->
    <section class="patient-area section-padding">
        <div class="container">
            <div class="row">
                <div class="col-lg-10 offset-lg-1 align-self-center">
                    <div class="appointment-form text-center mt-5 mt-lg-0">
                        <h3 class="mb-5">Contact Us</h3>
                        <form action="/" method="POST">
                            <div class="form-group">
                                <input type="text" placeholder="Your Name" name="name" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Your Name'" required>
                            </div>
                            <div class="form-group">
                                <input type="tel" placeholder="Contact No" name="contact" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Contact No'" required> 
                            </div>
                            <div class="form-group">
                                <textarea  cols="20" rows="7" name="comment"  placeholder="Message" onfocus="this.placeholder = ''" onblur="this.placeholder = 'Message'" required></textarea>
                            </div>
                            <div class="row">
                                <div class="col-sm-12 form-group">
                                  <button class="btn btn-default template-btn pull-right" type="submit">Send</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Patient Area Starts -->

